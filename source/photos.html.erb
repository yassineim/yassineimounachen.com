---
title: Photos
---

<div class="main photos">
  <div class="column">
    <%
      left_column_images = [
        { file_name: "mosque", description: "Hassan II Mosque model at CMN" },
        { file_name: "guitar", description: "Guitar at home" },
        { file_name: "sharks", description: "Aquarium at Morocco Mall" },
        { file_name: "bench", description: "Bench at Ain Almou, Ahfir region" },
      ]
    %>
    <% left_column_images.each do |image| %>
      <% link_to "images/full/" + image[:file_name] + ".jpg" do %>
        <%= image_tag "images/medium/" + image[:file_name] + "-medium.jpg", :alt => image[:description] %>
      <% end %>
    <% end %>
  </div>
  <div class="column">
    <%
      right_column_images = [
        { file_name: "sunset", description: "Sunset at SaÃ¯dia Beach" },
        { file_name: "contre-jour", description: "Contre-jour at Ain Almou" },
        { file_name: "fantasia", description: "Fantasia at Sidi Yahya, Oujda" },
      ]
    %>
    <% right_column_images.each do |image| %>
      <% link_to "images/full/" + image[:file_name] + ".jpg" do %>
        <%= image_tag "images/medium/" + image[:file_name] + "-medium.jpg", :alt => image[:description] %>
      <% end %>
    <% end %>
  </div>
</div>
